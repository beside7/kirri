{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {  
    "baseUrl": "./src",
    "target": "es5",   
    "module": "commonjs",
    "allowJs": true,    
    "checkJs": false,    
    "jsx": "react-native",
    "strict": true, 
    "skipLibCheck": true,    
    "forceConsistentCasingInFileNames": true,
  },
  "include": [
    "src"
  ],
  "exclude": [
    "node_modules"
  ]
}
